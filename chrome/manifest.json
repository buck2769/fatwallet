{
	"name": "YouTuber",
	"version": "0.1",
	"manifest_version": 2,
  //"background": {
    //"scripts": [ "underscore.js", "jquery.js", "vendors/pusher.js", "background.js" ],
    //"persistent": true
  //},
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": [ 
        "jquery.js",
        "application.js"
        //"bootstrap.js"
			],
      "css": [
        "toolbar.css"
      ]
		}
	],
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png"
    }
  },
  "web_accessible_resources": [
    "dollar-sign.png",
    "cancel.png"
  ],
  "permissions": [
    "tabs",
    "http://*.fatwallet.com/*",
    "webRequest"
  ],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
