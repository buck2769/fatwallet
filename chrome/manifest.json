{
	"name": "FatWallet",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": [ 
        "jquery.js",
        "application.js",
        "bootstrap.js"
			],
      "css": [
        "toolbar.css"
      ]
		}
	],
  "web_accessible_resources": [
    "dollar-sign.png",
    "cancel.png"
  ],
	"content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}
